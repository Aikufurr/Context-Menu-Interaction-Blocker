{
  "name": "Context Menu Interaction Blocker",
  "description": "Blocks interaction with the webpage when opening the context menu",
  "version": "1.2",
  "homepage_url": "https://aikufurr.xyz",
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "<all_urls>",
    "tabs",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_specific_settings": {
    "gecko": {
      "update_url": "https://raw.githubusercontent.com/Aikufurr/Context-Menu-Interaction-Blocker/main/updates.json",
      "id": "{8d1d15fe-e7e5-47b4-b480-6f503433be49}"
    }
  }
}
